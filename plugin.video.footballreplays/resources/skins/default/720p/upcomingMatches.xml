				<onleft>32506</onleft>
				<onright>32506</onright>
				<onup>32502</onup>
				<ondown>32505</ondown>
				<animation effect="slide" end="-120,0" time="0" condition="Window.Close">Conditional</animation>
				<visible>system.getbool(input.enablemouse)</visible>
			</control>
